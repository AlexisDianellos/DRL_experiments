      "source": [
        "import numpy as np\n",
        "import matplotlib.pylab as plt\n",
        "\n",
        "# Safe Analysis Block\n",
        "if len(all_returns) > 0:\n",
        "    # Truncate to match averaging window\n",
        "    remainder = len(all_returns) % configs.num_episodes_to_average\n",
        "    if remainder != 0:\n",
        "        clean_returns = np.array(all_returns[:-remainder])\n",
        "    else:\n",
        "        clean_returns = np.array(all_returns)\n",
        "\n",
        "    if len(clean_returns) > 0:\n",
        "        # Smooth\n",
        "        smoothed = np.average(clean_returns.reshape(-1, configs.num_episodes_to_average), axis=1)\n",
        "\n",
        "        print(f\"Total episodes: {len(all_returns)}\")\n",
        "        print(f\"Mean Return (Smoothed): {np.mean(smoothed):.2f}\")\n",
        "        print(f\"Max Return (Smoothed): {np.max(smoothed):.2f}\")\n",
        "\n",
        "        plt.figure(figsize=(10,5))\n",
        "        plt.plot(smoothed)\n",
        "        plt.title(\"PPO Training Progress\")\n",
        "        plt.xlabel(\"Updates (Averaged)\")\n",
        "        plt.ylabel(\"Return\")\n",
        "        plt.show()\n",
        "    else:\n",
        "        print(\"Not enough data points for smoothing.\")\n",
        "else:\n",
        "    print(\"Still no returns collected. Check if max_steps in the environment is preventing completion.\")"
      ]
    },
