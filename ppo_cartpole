      "id": "e14bf543",
      "metadata": {
        "id": "e14bf543"
      },
      "outputs": [],
      "source": [
        "agent = Agent(\n",
        "    env_observation_dim=envs.single_observation_space.shape[0],\n",
        "    action_space_dim=envs.single_action_space.n\n",
        ").to(device)\n",
        "\n",
        "optimizer = optim.Adam(agent.parameters(), lr=configs.learning_rate)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 30,
      "id": "56b3d5ca",
      "metadata": {
        "colab": {
